# Example secrets configuration for Production environment
# DO NOT commit this file with real values to version control
# Copy this file to secrets.tfvars and add your actual values

# Database credentials
master_username = "postgres"
master_password = "YOUR_PROD_DB_PASSWORD_HERE"

# Application API key
api_key = "YOUR_PROD_API_KEY_HERE"

# AWS Region (optional, defaults to us-east-1)
aws_region = "us-east-1"
